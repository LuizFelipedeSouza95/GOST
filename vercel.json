{
    "framework": "vite",
    "buildCommand": "yarn build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/((?!api).*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/**/*.{js,ts}": {
            "includeFiles": "src/entities/**/*"
        }
    }
}