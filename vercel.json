{
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/((?!api).*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/**/*": {
            "runtime": "nodejs22.x",
            "includeFiles": "dist/server/**/*"
        }
    }
}