{
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/((?!api).*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/**/*": {
            "runtime": "@vercel/node@2.0.0",
            "includeFiles": "dist/server/**/*"
        }
    }
}