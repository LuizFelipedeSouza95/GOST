{
    "name": "gost-site",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "engines": {
        "node": "22.x"
    },
    "scripts": {
        "dev": "vite",
        "build": "npm run build:server && vite build",
        "build:server": "tsc -p tsconfig.server.json",
        "preview": "vite preview --strictPort --port 5173",
        "migration:create": "tsx ./node_modules/@mikro-orm/cli/cli.js migration:create",
        "migration:up": "tsx ./node_modules/@mikro-orm/cli/cli.js migration:up",
        "migration:down": "tsx ./node_modules/@mikro-orm/cli/cli.js migration:down"
    },
    "resolutions": {
        "strip-ansi": "6.0.1"
    },
    "dependencies": {
        "@mikro-orm/cli": "^6.5.9",
        "@mikro-orm/core": "^6.5.9",
        "@mikro-orm/migrations": "^6.5.9",
        "@mikro-orm/postgresql": "^6.5.9",
        "@mikro-orm/reflection": "^6.5.9",
        "@types/uuid": "^11.0.0",
        "dotenv": "^17.2.3",
        "pg": "^8.13.1",
        "google-auth-library": "^9.14.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "reflect-metadata": "^0.2.2",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@types/node": "^24.10.0",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react": "4.3.2",
        "autoprefixer": "^10.4.20",
        "postcss": "^8.4.49",
        "tsx": "^4.19.2",
        "tailwindcss": "^3.4.14",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.3",
        "vite": "^5.4.10"
    }
}